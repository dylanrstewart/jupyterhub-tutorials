MEM_LIMIT=512M

# CPU limit per user container (fractional cores, e.g., 0.5, 1.0, 2.0)
CPU_LIMIT=1.0

# Docker Compose project name (affects network naming)
COMPOSE_PROJECT_NAME=rlm-jupyter
